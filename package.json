{
  "name": "starterkit-mfehost-motifreact-ts-fabric",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@azure/msal-browser": "3.6.0",
    "@azure/msal-react": "2.0.5",
    "@ey-xd/motif-react": "6.2.1",
    "@ey/mfe-ui-core": "1.25.0",
    "@microsoft/applicationinsights-web": "3.0.5",
    "axios": "^1.6.2",
    "bootstrap": "^5.2.3",
    "file-loader": "^6.2.0",
    "history": "5.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "redux-micro-frontend": "1.3.0"
  },
  "scripts": {
    "start": "env-cmd -f environments/.env craco start",
    "start:local": "env-cmd -f environments/.env.local craco start",
    "build": "env-cmd -f environments/.env.deployment craco build",
    "test": "craco test --transformIgnorePatterns \"node_modules/(?!@ey/mfe-ui-core)/\" --watchAll=false --env=jsdom --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "7.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "craco-alias": "3.0.1",
    "env-cmd": "^10.1.0",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "react-scripts": "5.0.1",
    "sass": "^1.67.0",
    "sass-loader": "^13.3.2",
    "source-map-loader": "4.0.1",
    "styled-components": "6.0.8",
    "ts-jest": "29.1.4",
    "typescript": "^4.9.5",
    "uuid": "9.0.1",
    "web-vitals": "^3.5.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "index.tsx",
      "bootstrap.ts",
      "reportWebVitals.ts"
    ],
    "coverageReporters": [
      "cobertura",
      "html",
      "text",
      "text-summary"
    ]
  }
}
